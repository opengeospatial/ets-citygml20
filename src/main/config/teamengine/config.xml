<!-- Add this entry to TE_BASE/config.xml (//organization/standard) -->
<organization>
  <name>OGC</name>
  <standard>
    <name>CityGML 2.0</name>
    <version>
      <name>${spec-version}</name>
      <suite>
        <namespace-uri>http://www.opengis.net/cite/citygml20</namespace-uri>
        <prefix>tns</prefix>
        <local-name>ets-citygml20-${version}</local-name>
        <title>ets-citygml20 Conformance Test Suite</title>
        <description>Describe scope of testing.</description>
        <!-- Add baisc conformance test suites
        <BasicConformanceClasses>
          <conformanceClass>Core</conformanceClass>
        </BasicConformanceClasses>
        -->
      </suite>
      <revision>
        <name>${project.version}</name>
        <status>Beta</status>
        <sources>
          <source>citygml20/${spec-version}/ctl</source>
        </sources>
        <webdir>citygml20/${spec-version}/site</webdir>
      </revision>
    </version>
  </standard>
